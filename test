		if (mc >= doo && mc >= or && mc >= ge && mc >= ph && mc >= re && mc >= so && mc >= som && mc >= tr && mc >= ba && mc >= ha && mc >= ju && mc >= me && mc >= to && mc >= wi && mc >= dv && mc >= rei && mc >= ro && mc >= win && mc >= za && mc >= an && mc >= lu && mc >= mer && mc >= sy && mc >= ze){
			System.out.println("You got McCree!");
		}
		if (ph >= doo && ph >= or && ph >= ge && ph >= mc && ph >= re && ph >= so && ph >= som && ph >= tr && ph >= ba && ph >= ha && ph >= ju && ph >= me && ph >= to && ph >= wi && ph >= dv && ph >= rei && ph >= ro && ph >= win && ph >= za && ph >= an && ph >= lu && ph >= mer && ph >= sy && ph >= ze){
			System.out.println("You got Phara!");
		}
		if (re >= doo && re >= or && re >= ge && re >= mc && re >= ph && re >= so && re >= som && re >= tr && re >= ba && re >= ha && re >= ju && re >= me && re >= to && re >= wi && re >= dv && re >= rei && re >= ro && re >= win && re >= za && re >= an && re >= lu && re >= mer && re >= sy && re >= ze){
			System.out.println("You got Reaper!");
		}
		if (so >= doo && so >= or && so >= ge && so >= mc && so >= ph && so >= re && so >= so && so >= som && so >= tr && so >= ba && so >= ha && so >= ju && so >= me && so >= to && so >= wi && so >= dv && so >= rei && so >= ro && so >= win && so >= za && so >= an && so >= lu && so >= mer && so >= sy && so >= ze){
			System.out.println("You got Soldier 76!");
		}
		if (som >= doo && som >= or && som >= ge && som >= mc && som >= ph && som >= re && som >= so && som >= som && som >= tr && som >= ba && som >= ha && som >= ju && som >= me && som >= to && som >= wi && som >= dv && som >= rei && som >= ro && som >= win && som >= za && som >= an && som >= lu && som >= mer && som >= sy && som >= ze){
			System.out.println("You got Sombra!");
		}
		if (tr >= doo && tr >= or && tr >= ge && tr >= mc && tr >= ph && tr >= re && tr >= so && tr >= som && tr >= tr && tr >= ba && tr >= ha && tr >= ju && tr >= me && tr >= to && tr >= wi && tr >= dv && tr >= rei && tr >= ro && tr >= win && tr >= za && tr >= an && tr >= lu && tr >= mer && tr >= sy && tr >= ze){
			System.out.println("You got Tracer!");
		}
		if (ba >= doo && ba >= or && ba >= ge && ba >= mc && ba >= ph && ba >= re && ba >= so && ba >= som && ba >= tr && ba >= ba && ba >= ha && ba >= ju && ba >= me && ba >= to && ba >= wi && ba >= dv && ba >= rei && ba >= ro && ba >= win && ba >= za && ba >= an && ba >= lu && ba >= mer && ba >= sy && ba >= ze){
			System.out.println("You got Bastion!");
		}
		if (ha >= doo && ha >= or && ha >= ge && ha >= mc && ha >= ph && ha >= re && ha >= so && ha >= som && ha >= tr && ha >= ba && ha >= ha && ha >= ju && ha >= me && ha >= to && ha >= wi && ha >= dv && ha >= rei && ha >= ro && ha >= win && ha >= za && ha >= an && ha >= lu && ha >= mer && ha >= sy && ha >= ze){
			System.out.println("You got Hanzo!");
		}
		if (ju >= doo && ju >= or && ju >= ge && ju >= mc && ju >= ph && ju >= re && ju >= so && ju >= som && ju >= tr && ju >= ba && ju >= ha && ju >= ju && ju >= me && ju >= to && ju >= wi && ju >= dv && ju >= rei && ju >= ro && ju >= win && ju >= za && ju >= an && ju >= lu && ju >= mer && ju >= sy && ju >= ze){
			System.out.println("You got Junkrat!");
		}
		if (me >= doo && me >= or && me >= ge && me >= mc && me >= ph && me >= re && me >= so && me >= som && me >= tr && me >= ba && me >= ha && me >= ju && me >= me && me >= to && me >= wi && me >= dv && me >= rei && me >= ro && me >= win && me >= za && me >= an && me >= lu && me >= mer && me >= sy && me >= ze){
			System.out.println("You got Mei!");
		}
		if (to >= doo && to >= or && to >= ge && to >= mc && to >= ph && to >= re && to >= so && to >= som && to >= tr && to >= ba && to >= ha && to >= ju && to >= me && to >= to && to >= wi && to >= dv && to >= rei && to >= ro && to >= win && to >= za && to >= an && to >= lu && to >= mer && to >= sy && to >= ze){
			System.out.println("You got Torbjörn!");
		}
		if (wi >= doo && wi >= or && wi >= ge && wi >= mc && wi >= ph && wi >= re && wi >= so && wi >= som && wi >= tr && wi >= ba && wi >= ha && wi >= ju && wi >= me && wi >= to && wi >= wi && wi >= dv && wi >= rei && wi >= ro && wi >= win && wi >= za && wi >= an && wi >= lu && wi >= mer && wi >= sy && wi >= ze){
			System.out.println("You got Widowmaker!");
		}
		if (dv >= doo && dv >= or && dv >= ge && dv >= mc && dv >= ph && dv >= re && dv >= so && dv >= som && dv >= tr && dv >= ba && dv >= ha && dv >= ju && dv >= me && dv >= to && dv >= wi && dv >= dv && dv >= rei && dv >= ro && dv >= win && dv >= za && dv >= an && dv >= lu && dv >= mer && dv >= sy && dv >= ze){
			System.out.println("You got D.va!");
		}
		if (rei >= doo && rei >= or && rei >= ge && rei >= mc && rei >= ph && rei >= re && rei >= so && rei >= som && rei >= tr && rei >= ba && rei >= ha && rei >= ju && rei >= me && rei >= to && rei >= wi && rei >= dv && rei >= rei && rei >= ro && rei >= win && rei >= za && rei >= an && rei >= lu && rei >= mer && rei >= sy && rei >= ze){
			System.out.println("You got Reinhardt!");
		}
		if (ro >= doo && ro >= or && ro >= ge && ro >= mc && ro >= ph && ro >= re && ro >= so && ro >= som && ro >= tr && ro >= ba && ro >= ha && ro >= ju && ro >= me && ro >= to && ro >= wi && ro >= dv && ro >= rei && ro >= ro && ro >= win && ro >= za && ro >= an && ro >= lu && ro >= mer && ro >= sy && ro >= ze){
			System.out.println("You got Roadhog!");
		}
		if (win >= doo && win >= or && win >= ge && win >= mc && win >= ph && win >= re && win >= so && win >= som && win >= tr && win >= ba && win >= ha && win >= ju && win >= me && win >= to && win >= wi && win >= dv && win >= rei && win >= ro && win >= win && win >= za && win >= an && win >= lu && win >= mer && win >= sy && win >= ze){
			System.out.println("You got Winston!");
		}
		if (za >= doo && za >= or && za >= ge && za >= mc && za >= ph && za >= re && za >= so && za >= som && za >= tr && za >= ba && za >= ha && za >= ju && za >= me && za >= to && za >= wi && za >= dv && za >= rei && za >= ro && za >= win && za >= za && za >= an && za >= lu && za >= mer && za >= sy && za >= ze){
			System.out.println("You got Zarya!");
		}
		if (an >= doo && an >= or && an >= ge && an >= mc && an >= ph && an >= re && an >= so && an >= som && an >= tr && an >= ba && an >= ha && an >= ju && an >= me && an >= to && an >= wi && an >= dv && an >= rei && an >= ro && an >= win && an >= za && an >= an && an >= lu && an >= mer && an >= sy && an >= ze){
			System.out.println("You got Ana!");
		}
		if (lu >= doo && lu >= or && lu >= ge && lu >= mc && lu >= ph && lu >= re && lu >= so && lu >= som && lu >= tr && lu >= ba && lu >= ha && lu >= ju && lu >= me && lu >= to && lu >= wi && lu >= dv && lu >= rei && lu >= ro && lu >= win && lu >= za && lu >= an && lu >= lu && lu >= mer && lu >= sy && lu >= ze){
			System.out.println("You got Lúcio!");
		}
		if (mer >= doo && mer >= or && mer >= ge && mer >= mc && mer >= ph && mer >= re && mer >= so && mer >= som && mer >= tr && mer >= ba && mer >= ha && mer >= ju && mer >= me && mer >= to && mer >= wi && mer >= dv && mer >= rei && mer >= ro && mer >= win && mer >= za && mer >= an && mer >= lu && mer >= mer && mer >= sy && mer >= ze){
			System.out.println("You got Mercy!");
		}
		if (sy >= doo && sy >= or && sy >= ge && sy >= mc && sy >= ph && sy >= re && sy >= so && sy >= som && sy >= tr && sy >= ba && sy >= ha && sy >= ju && sy >= me && sy >= to && sy >= wi && sy >= dv && sy >= rei && sy >= ro && sy >= win && sy >= za && sy >= an && sy >= lu && sy >= mer && sy >= sy && sy >= ze){
			System.out.println("You got Symmetra!");
		}
		if (ze >= doo && ze >= or && ze >= ge && ze >= mc && ze >= ph && ze >= re && ze >= so && ze >= som && ze >= tr && ze >= ba && ze >= ha && ze >= ju && ze >= me && ze >= to && ze >= wi && ze >= dv && ze >= rei && ze >= ro && ze >= win && ze >= za && ze >= an && ze >= lu && ze >= mer && ze >= sy && ze >= ze){
			System.out.println("You got Zenyatta!");
		}
		if (or >= doo && or >= or && or >= ge && or >= mc && or >= ph && or >= re && or >= so && or >= som && or >= tr && or >= ba && or >= ha && or >= ju && or >= me && or >= to && or >= wi && or >= dv && or >= rei && or >= ro && or >= win && or >= za && or >= an && or >= lu && or >= mer && or >= sy && or >= ze){
			System.out.println("You got Orisa!");
		}
		System.out.println("If you want to see how high each character scored say tally, if not, congrats on getting your character! (:");
	
		choice = scan.next();
		if (choice.equals("tally")){
			System.out.println("________DPS_______");
			System.out.println("Doomfist was: " + doo);
			System.out.println("Gengi was: " + ge);
			System.out.println("McCree was: " + mc);
			System.out.println("Phara was: " + ph);
			System.out.println("Reaper was: " + re);
			System.out.println("Soldier 76 was: " + so);
			System.out.println("Sombra was: " + som);
			System.out.println("Tracer was: " + tr);
			System.out.println("Bastion was: " + ba);
			System.out.println("Hanzo was: " + ha);
			System.out.println("Junkrat was: " + ju);
			System.out.println("Mei was: " + me);
			System.out.println("Torbjörn was: " + to);
			System.out.println("Widowmaker was: " + wi);
			System.out.println("_______TANKS_______");
			System.out.println("D.va was: " + dv);
			System.out.println("Reinhardt was: " + rei);
			System.out.println("Roadhog was: " + ro);
			System.out.println("Winston was: " + win);
			System.out.println("Zarya was: " + za);
			System.out.println("Orisa was: " + or);
			System.out.println("______SUPPORT______");
			System.out.println("Ana was: " + an);
			System.out.println("Lúcio was: " + lu);
			System.out.println("Mercy was: " + mer);
			System.out.println("Symmetra was: " + sy);
			System.out.println("Zenyatta was: " + ze);
		}
	}

